{
  "private": true,
  "name": "auth-backend",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently --raw 'pnpm start:typecheck' 'pnpm web:compile'",
    "web:compile": "ts-node-dev --no-notify --ignore-watch node_modules src/apps/auth-web.ts",
    "start:typecheck": "tsc -w --noEmit",
    "build": "rm -rf dist && webpack --config ./webpack.config.js",
    "rebuild-models": "ts-node ./tools/rebuild-models.ts",
    "test": "nyc mocha --exit"
  },
  "dependencies": {
    "oauth2-server": "^3.1.1"
  },
  "devDependencies": {
    "@types/oauth2-server": "^3.0.13"
  }
}
